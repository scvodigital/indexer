{
  "domains": [
    "wp-indexer.scvo.local"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../../../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/scvo/",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    },
    "commonTasks": "{:./data/common-tasks.json}"
  }
}